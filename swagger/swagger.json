{
    "openapi": "3.0.0",
    "info": {
      "title": "Restaurant Reservation API Documentation",
      "version": "1.0.0",
      "description": "API for managing restaurant table reservations, built with Node.js, Express, and MongoDB. Includes Google Authentication for secure access."
    },
    "servers": [
      {
        "url": "http://localhost:3000",
        "description": "Local development server"
      }
      ],
    "tags": [
      {
        "name": "Tables",
        "description": "Operations related to restaurant tables"
      },
      {
        "name": "Reservations",
        "description": "Operations related to table reservations"
      },
      {
        "name": "Restaurant Info",
        "description": "Operations for managing general restaurant information"
      },
      {
        "name": "Authentication",
        "description": "Authentication endpoints (Google OAuth 2.0)"
      }
    ],
    "components": {
      "securitySchemes": {
        "cookieAuth": {
          "type": "apiKey",
          "in": "cookie",
          "name": "connect.sid",
          "description": "Session cookie for Google Authentication. After successful Google login, a `connect.sid` cookie is set, which is used for subsequent authenticated requests."
        }
      },
      "schemas": {
        "ErrorResponse": {
          "type": "object",
          "properties": {
            "message": {
              "type": "string",
              "description": "Error message describing the issue."
            }
          },
          "example": {
            "message": "Validation error: Customer name is required."
          }
        },
        "Table": {
          "type": "object",
          "properties": {
            "_id": {
              "type": "string",
              "description": "Unique ID of the table (MongoDB ObjectId)",
              "readOnly": true
            },
            "tableNumber": {
              "type": "string",
              "description": "Unique identifier for the table (e.g., 'T1', '102')",
              "example": "T1"
            },
            "capacity": {
              "type": "integer",
              "format": "int32",
              "description": "Maximum capacity of the table",
              "example": 4
            },
            "location": {
              "type": "string",
              "description": "Location of the table within the restaurant (optional)",
              "example": "Indoor, near window"
            },
            "isAvailable": {
              "type": "boolean",
              "description": "Indicates if the table is currently available for reservations (optional, default: true)",
              "example": true
            }
          },
          "required": [
            "tableNumber",
            "capacity"
          ],
          "example": {
            "tableNumber": "T1",
            "capacity": 4,
            "location": "Indoor",
            "isAvailable": true
          }
        },
        "Reservation": {
          "type": "object",
          "properties": {
            "_id": {
              "type": "string",
              "description": "Unique ID of the reservation (MongoDB ObjectId)",
              "readOnly": true
            },
            "tableId": {
              "type": "string",
              "description": "ID of the table reserved (reference to Table collection)",
              "example": "64d8c2e4f7b3e2a0c5d4e6f8"
            },
            "customerName": {
              "type": "string",
              "description": "Name of the customer making the reservation",
              "example": "John Doe"
            },
            "customerPhone": {
              "type": "string",
              "description": "Phone number of the customer (optional)",
              "example": "555-123-4567"
            },
            "reservationTime": {
              "type": "string",
              "format": "date-time",
              "description": "Date and time of the reservation (ISO 8601 format)",
              "example": "2024-08-10T19:00:00Z"
            },
            "partySize": {
              "type": "integer",
              "format": "int32",
              "description": "Number of people in the party",
              "example": 2
            },
            "status": {
              "type": "string",
              "description": "Status of the reservation",
              "enum": ["Pending", "Confirmed", "Seated", "Cancelled"],
              "default": "Pending",
              "example": "Confirmed"
            },
            "notes": {
              "type": "string",
              "description": "Additional notes for the reservation (optional)",
              "example": "Birthday celebration"
            }
          },
          "required": [
            "tableId",
            "customerName",
            "reservationTime",
            "partySize"
          ],
          "example": {
            "tableId": "64d8c2e4f7b3e2a0c5d4e6f8",
            "customerName": "John Doe",
            "reservationTime": "2024-08-10T19:00:00Z",
            "partySize": 2,
            "status": "Pending",
            "notes": "Vegetarian guest"
          }
        },
        "RestaurantInfo": {
          "type": "object",
          "properties": {
            "_id": {
              "type": "string",
              "description": "Unique ID of the restaurant info document (MongoDB ObjectId)",
              "readOnly": true
            },
            "name": {
              "type": "string",
              "description": "Name of the restaurant",
              "example": "The Cozy Bistro"
            },
            "address": {
              "type": "string",
              "description": "Restaurant address",
              "example": "123 Main Street, Cityville"
            },
            "phoneNumber": {
              "type": "string",
              "description": "Restaurant phone number",
              "example": "555-555-5555"
            },
            "openingHours": {
              "type": "string",
              "description": "Restaurant opening hours",
              "example": "Mon-Sun: 11:00 AM - 10:00 PM"
            },
            "website": {
              "type": "string",
              "format": "url",
              "description": "Restaurant website URL (optional)",
              "example": "https://www.cozybistro.com"
            }
          },
          "required": [
            "name"
          ],
          "example": {
            "name": "The Cozy Bistro",
            "address": "123 Main Street, Cityville",
            "phoneNumber": "555-555-5555",
            "openingHours": "Mon-Sun: 11:00 AM - 10:00 PM",
            "website": "https://www.cozybistro.com"
          }
        },
        "User": {
          "type": "object",
          "properties": {
            "_id": {
              "type": "string",
              "description": "Unique ID of the user (MongoDB ObjectId)",
              "readOnly": true
            },
            "googleId": {
              "type": "string",
              "description": "Google ID of the user",
              "example": "123456789012345678901"
            },
            "email": {
              "type": "string",
              "format": "email",
              "description": "User's email address",
              "example": "user@example.com"
            },
            "name": {
              "type": "string",
              "description": "User's full name",
              "example": "John Doe"
            }
          },
          "required": [
            "googleId",
            "email",
            "name"
          ],
          "example": {
            "googleId": "123456789012345678901",
            "email": "user@example.com",
            "name": "John Doe"
          }
        },
        "ReservationStatus": {
          "type": "object",
          "properties": {
            "_id": {
              "type": "string",
              "description": "Unique ID of the reservation status (MongoDB ObjectId)",
              "readOnly": true
            },
            "name": {
              "type": "string",
              "description": "Name of the status (e.g., 'Pending', 'Confirmed')",
              "example": "Confirmed"
            },
            "description": {
              "type": "string",
              "description": "Description of the status (optional)",
              "example": "Reservation has been confirmed by staff"
            }
          },
          "required": [
            "name"
          ],
          "example": {
            "name": "Confirmed",
            "description": "Reservation has been confirmed by staff"
          }
        }
      }
    },
